{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "indent": ["error", 2],
    "keyword-spacing": "error",
    "no-undef": 0,
    "no-unsued-vars": 0,
    "quotes": ["error", "single"],
    "react/prop-types": 0,
    "react/display-name": 0,
    "semi": ["error", "always"],
    "space-before-blocks": "error",
    "space-before-function-paren": "error"
  }
}
